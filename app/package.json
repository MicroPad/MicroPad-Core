{
  "name": "micropad",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "preinstall": "python3 ../libs/build-libs.py && ./get_precache_files.py > src/extraPrecacheFiles.ts",
    "prestart": "tsc -p _build/tsconfig.json",
    "start": "NODE_ENV='' node _build/build.js",
    "prebuild": "tsc -p _build/tsconfig.json",
    "build": "NODE_ENV=production node _build/build.js",
    "test": "TZ=NZ jest",
    "typecheck": "tsc --noEmit --p ./tsconfig.json",
    "typecheck:watch": "tsc --noEmit --watch -p ./tsconfig.json",
    "lint": "eslint src/"
  },
  "dependencies": {
    "@fontsource/abeezee": "^4.4.5",
    "@sentry/integrations": "^6.6.0",
    "@sentry/react": "^6.6.0",
    "@sentry/tracing": "^6.6.0",
    "browserslist-useragent-regexp": "^3.0.0",
    "date-fns": "^2.21.3",
    "deep-freeze": "~0.0.1",
    "fend-wasm-web": "~0.1.26",
    "json-stringify-safe": "^5.0.1",
    "jszip": "^3.1.5",
    "localforage": "^1.5.6",
    "material-icons-font": "^2.1.0",
    "materialize-css": "^1.0.0",
    "mousetrap": "^1.6.1",
    "opus-recorder": "^4.1.0",
    "paste-image": "^0.0.3",
    "pdfobject": "^2.1.1",
    "re-resizable": "^6.9.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.3.1",
    "react-materialize": "^3.9.9",
    "react-redux": "^7.1.0",
    "react-select": "^5.2.1",
    "react-treeview": "^0.4.7",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-observable": "^0.19.0",
    "redux-typescript-actions": "^1.3.0",
    "rxjs": "^6.6.6",
    "rxjs-compat": "^6.6.6",
    "save-as": "^0.1.8",
    "semver": "^7.3.4",
    "showdown": "^1.8.6",
    "timers-browserify": "^2.0.12",
    "typescript": "~4.4.3",
    "upad-parse": "^7.3.0",
    "vex-dialog": "^1.1.0",
    "vex-js": "^4.1.0",
    "workbox-core": "^6.1.5",
    "workbox-expiration": "^6.1.5",
    "workbox-precaching": "^6.1.5",
    "workbox-routing": "^6.1.5",
    "workbox-strategies": "^6.1.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:jest-playwright/recommended"
    ],
    "ignorePatterns": ["*.js"],
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "@typescript-eslint/no-use-before-define": "off",
          "eqeqeq": [
            "error",
            "smart"
          ],
          "no-multi-spaces": "error",
          "array-bracket-spacing": [
            "error",
            "never"
          ],
          "object-curly-spacing": [
            "error",
            "always"
          ],
          "key-spacing": "error",
          "@typescript-eslint/prefer-ts-expect-error": "warn"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      "chrome >= 80",
      "electron >= 12",
      "edge >= 80",
      "firefox >= 78",
      "safari >= 13",
      "ios_saf >= 13",
      "unreleased versions"
    ],
    "development": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version",
      "last 2 electron version"
    ]
  },
  "devDependencies": {
    "@types/html-minifier": "^4.0.1",
    "@types/jest": "^27.0.1",
    "@types/jest-image-snapshot": "^4.3.0",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/mousetrap": "^1.6.7",
    "@types/node": "^16.9.2",
    "@types/react-redux": "^7.1.2",
    "@types/semver": "^7.3.4",
    "@types/showdown": "^1.7.3",
    "@types/workbox-build": "^5.0.1",
    "@types/deep-freeze": "^0.1.2",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "browserslist": "^4.16.6",
    "core-js": "^3.17.3",
    "esbuild": "^0.13.2",
    "esbuild-plugin-browserslist": "^0.2.1",
    "esbuild-plugin-manifest": "^0.4.6",
    "eslint": "^8.2.0",
    "eslint-config-react-app": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-jest-playwright": "~0.6.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "html-minifier": "^4.0.0",
    "jest": "^27.2.0",
    "jest-circus": "^27.2.0",
    "jest-environment-node": "^27.2.0",
    "jest-image-snapshot": "^4.5.1",
    "jest-playwright-preset": "^1.7.0",
    "jest-runner": "^27.2.0",
    "playwright": "^1.14.1",
    "servor": "^4.0.2",
    "ts-jest": "^27.0.5",
    "workbox-build": "^6.1.5"
  }
}
