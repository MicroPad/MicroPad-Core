{
  "name": "micropad",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "preinstall": "python3 ../libs/build-libs.py && ./get_precache_files.py > src/extraPrecacheFiles.ts",
    "prestart": "tsc -p _build/tsconfig.json",
    "start": "NODE_ENV='' node _build/build.js",
    "prebuild": "tsc -p _build/tsconfig.json",
    "build": "NODE_ENV=production node _build/build.js",
    "test": "TZ=NZ jest",
    "typecheck": "tsc --noEmit --p ./tsconfig.json",
    "typecheck:watch": "tsc --noEmit --watch -p ./tsconfig.json",
    "lint": "eslint src/"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@fontsource/abeezee": "^4.4.5",
    "@nick_webster/photon": "^0.3.1",
    "@redux-devtools/extension": "^3.2.1",
    "@reduxjs/toolkit": "^1.8.1",
    "@sentry/integrations": "^6.19.3",
    "@sentry/react": "^6.19.3",
    "@sentry/tracing": "^6.19.3",
    "browserslist-useragent-regexp": "^3.0.0",
    "date-fns": "^2.21.3",
    "deep-freeze": "~0.0.1",
    "fend-wasm-web": "^1.0.1",
    "json-stringify-safe": "^5.0.1",
    "jszip": "^3.1.5",
    "localforage": "^1.5.6",
    "material-icons-font": "^2.1.0",
    "materialize-css": "^1.0.0",
    "mousetrap": "^1.6.1",
    "opus-recorder": "^4.1.0",
    "pdfobject": "^2.1.1",
    "re-resizable": "^6.9.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.4.4",
    "react-materialize": "^3.9.9",
    "react-redux": "^8.0.1",
    "react-select": "^5.2.1",
    "react-treeview": "~0.4.7",
    "redux-observable": "^2.0.0",
    "rxjs": "^7.5.1",
    "save-as": "^0.1.8",
    "semver": "^7.3.4",
    "showdown": "^1.9.1",
    "typescript": "~4.7.2",
    "typescript-fsa": "^3.0.0",
    "upad-parse": "^7.4.1",
    "vex-dialog": "^1.1.0",
    "vex-js": "^4.1.0",
    "workbox-core": "^6.1.5",
    "workbox-expiration": "^6.1.5",
    "workbox-precaching": "^6.1.5",
    "workbox-routing": "^6.1.5",
    "workbox-strategies": "^6.1.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:jest-playwright/recommended"
    ],
    "ignorePatterns": [
      "*.js"
    ],
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "@typescript-eslint/no-use-before-define": "off",
          "eqeqeq": [
            "error",
            "smart"
          ],
          "no-multi-spaces": "error",
          "array-bracket-spacing": [
            "error",
            "never"
          ],
          "object-curly-spacing": [
            "error",
            "always"
          ],
          "key-spacing": "error",
          "@typescript-eslint/prefer-ts-expect-error": "warn",
          "no-debugger": "error",
          "quotes": [
            "error",
            "single",
            {
              "allowTemplateLiterals": true
            }
          ],
          "jsx-quotes": [
            "error",
            "prefer-double"
          ],
          "no-script-url": "error",
          "no-eval": "error",
          "no-console": [
            "error",
            {
              "allow": [
                "warn",
                "error"
              ]
            }
          ]
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      "chrome >= 84",
      "electron >= 12",
      "edge >= 80",
      "firefox >= 78",
      "safari >= 13.1",
      "ios_saf >= 13.4",
      "unreleased versions"
    ],
    "development": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version",
      "last 2 electron version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-flow": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@types/deep-freeze": "^0.1.2",
    "@types/html-minifier": "^4.0.1",
    "@types/jest": "^27.0.1",
    "@types/jest-image-snapshot": "^4.3.0",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/mousetrap": "^1.6.7",
    "@types/node": "^17.0.8",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.15",
    "@types/semver": "^7.3.4",
    "@types/showdown": "^1.9.4",
    "@types/workbox-build": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "babel-eslint": "^10.1.0",
    "browserslist": "^4.16.6",
    "esbuild": "^0.14.10",
    "esbuild-plugin-browserslist": "^0.4.1",
    "esbuild-plugin-manifest": "^0.5.0",
    "eslint": "^8.2.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-jest-playwright": "~0.9.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "html-minifier": "^4.0.0",
    "jest": "^27.2.0",
    "jest-circus": "^27.2.0",
    "jest-environment-node": "^27.2.0",
    "jest-image-snapshot": "^4.5.1",
    "jest-junit": "^13.0.0",
    "jest-playwright-preset": "^1.7.0",
    "jest-runner": "^27.2.0",
    "playwright": "^1.14.1",
    "servor": "^4.0.2",
    "ts-jest": "^27.0.5",
    "workbox-build": "^6.1.5"
  },
  "resolutions": {
    "@types/react": "^17",
    "react": "^17",
    "redux": "^4"
  }
}
